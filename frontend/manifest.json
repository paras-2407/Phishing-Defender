{
    "manifest_version": 2,
    "name": "Systumm Projects Extension",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": {
        "16": "pic/icon16.png"
      },
      "default_popup": "index.html",
      "default_title": "Click to open the extension"
    },
    "icons": {
      "16": "pic/icon16.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["style.css"]
      }
    ]
  }
  